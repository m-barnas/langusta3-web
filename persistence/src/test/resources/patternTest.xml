<?xml version="1.0" encoding="UTF-8"?>

<pattern pos="" id="občan" parent="soused">
    <!--občan = občan, soused (-an, -al, -st, -it, -at, -ot, -aut, -ant, -ed, -id)-->
    <condition-list lemma="yes">
        <condition charno="-1" type="equal" value="n,l" />
        <condition charno="-2" type="equal" value="a" />
    </condition-list>

    <condition-list lemma="yes">
        <basechange no="2" operation="chop" charno="4" />
        <condition charno="-1" type="equal" value="t" />
        <basechange no="2" operation="chop" charno="5" />
        <condition charno="-2" type="equal" value="s,i,a,o" />
        <basechange no="2" operation="chop" charno="6" />
    </condition-list>

    <basechange no="2" operation="chop" charno="3" />

    <condition-list lemma="yes">
        <condition charno="-1" type="equal" value="t" />
        <condition charno="-2" type="equal" value="u,n" />
        <condition charno="-3" type="equal" value="a" />
    </condition-list>

    <basechange no="2" operation="chop" charno="2" />

    <condition-list lemma="yes">
        <condition charno="-1" type="equal" value="d" />
        <condition charno="-2" type="equal" value="e,i" />
    </condition-list>

    <basechange no="2" operation="chop" charno="1" />

    <form base="1" lemma="yes"><suffix></suffix><tag>k1gMnSc1</tag></form>
    <form base="1"><suffix>a</suffix><tag>k1gMnSc2</tag></form>
    <form base="1"><suffix>o,v,i</suffix><tag>k1gMnSc3-a</tag></form>
    <form base="1"><suffix>u</suffix><tag>k1gMnSc3-b</tag></form>
    <form base="1"><suffix>a</suffix><tag>k1gMnSc4</tag></form>
    <form base="1"><suffix>e</suffix><tag>k1gMnSc5</tag></form>
    <form base="1"><suffix>o,v,i</suffix><tag>k1gMnSc6-a</tag></form>
    <form base="1"><suffix>u</suffix><tag>k1gMnSc6-b</tag></form>
    <form base="1"><suffix>e,m</suffix><tag>k1gMnSc7</tag></form>
    <form base="1"><suffix>é</suffix><tag>k1gMnPc1</tag></form>
    <form base="1"><suffix>ů</suffix><tag>k1gMnPc2</tag></form>
    <form base="1"><suffix>ů,m</suffix><tag>k1gMnPc3</tag></form>
    <form base="1"><suffix>y</suffix><tag>k1gMnPc4</tag></form>
    <form base="1"><suffix>é</suffix><tag>k1gMnPc5</tag></form>
    <form base="1"><suffix>e,c,h</suffix><tag>k1gMnPc6</tag></form>
    <form base="1"><suffix>y</suffix><tag>k1gMnPc7</tag></form>
</pattern>